{
  "name": "X-Forwarded-For Location Selector",
  "options_page": "options.html",
  "background": {
    "scripts": [
        "background.bundle.js"
    ],
    "persistent": true
},
  "browser_action": {
    "default_popup": "options.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
},
"permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
